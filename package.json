  },
  "homepage": "https://github.com/LaizaBertelli/ebytr-back#readme",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "node-encoder": "^2.0.4",
    "nodemon": "^2.0.16",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "eslint": "^8.15.0"
  }
}
